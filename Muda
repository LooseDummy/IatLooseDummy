key1 = "r"
key2 = "e"
key3 = "h"
key4 = "u"
key5 = "b"
key6 = "e"
key7 = "g"
local player = game.Players.localPlayer
local mouse = player:GetMouse()
mouse.KeyDown:connect(function(key)
if key == key1 then
loadstring(game:HttpGet("https://raw.githubusercontent.com/LooseDummy/IatLooseDummy/refs/heads/main/Kkk"))()
elseif key == key2 then
loadstring(game:HttpGet("https://raw.githubusercontent.com/LooseDummy/IatLooseDummy/refs/heads/main/Jjj"))()
elseif key == key3 then
loadstring(game:HttpGet("https://raw.githubusercontent.com/LooseDummy/IatLooseDummy/refs/heads/main/Bsbsbd"))()
elseif key == key4 then
loadstring(game:HttpGet("https://raw.githubusercontent.com/LooseDummy/IatLooseDummy/refs/heads/main/Attack/3."))()
elseif key == key5 then
loadstring(game:HttpGet("https://raw.githubusercontent.com/LooseDummy/IatLooseDummy/refs/heads/main/Attack/3"))()
elseif key == key6 then
loadstring(game:HttpGet("https://raw.githubusercontent.com/LooseDummy/IatLooseDummy/refs/heads/main/Attack/1"))()
elseif key == key7 then
loadstring(game:HttpGet("https://raw.githubusercontent.com/LooseDummy/IatLooseDummy/refs/heads/main/Attack/2"))() 
